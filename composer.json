{
    "name": "jp3cki/yii2-extra-validator",
    "type": "yii2-extension",
    "description": "Validators for Yii2",
    "keywords": ["yii2", "extension", "validator", "recaptcha", "twitter"],
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php": ">= 7.1.0",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "jp3cki/mb_str_replace": "^4.0",
        "php-curl-class/php-curl-class": "^8.6.1",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "^3.5.2"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\validators\\": "src",
            "jp3cki\\yii2\\validators\\testsrc\\": "test/src"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\validators\\internal\\Bootstrap"
    },
    "config": {
        "platform": {
            "php": "7.1.33"
        }
    }
}
